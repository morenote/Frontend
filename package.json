{
  "name": "morenote",
  "version": "0.0.1",
  "description": "ng-zorro-antd admin panel front-end framework",
  "author": "cipchk <cipchk@qq.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-alain/ng-alain.git"
  },
  "bugs": {
    "url": "https://github.com/ng-alain/ng-alain/issues"
  },
  "homepage": "https://ng-alain.com",
  "license": "MIT",
  "keywords": [
    "delon",
    "antd",
    "ng-zorro-antd",
    "angular",
    "component",
    "scaffold"
  ],
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "ng": "ng",
    "start": "ng s -o --port 3201",
    "hmr": "ng s -o --hmr",
    "build": "pnpm run ng-high-memory build",
    "analyze": "pnpm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "lint": "pnpm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "npx stylelint 'src/**/*.less'",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "jest": "pnpm run test",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "prepare": "husky install",
    "create-config": "ts-jest config:init"
  },
  "dependencies": {
    "@angular/animations": "^18.1.1",
    "@angular/common": "^18.1.1",
    "@angular/compiler": "^18.1.1",
    "@angular/core": "^18.1.1",
    "@angular/forms": "^18.1.1",
    "@angular/platform-browser": "^18.1.1",
    "@angular/platform-browser-dynamic": "^18.1.1",
    "@angular/router": "^18.1.1",
    "@angular/service-worker": "^18.1.1",
    "@antv/g2": "^4.2.10",
    "@aomao/engine": "^2.9.26",
    "@delon/abc": "^18.0.2",
    "@delon/acl": "^18.0.2",
    "@delon/auth": "^18.0.2",
    "@delon/cache": "^18.0.2",
    "@delon/chart": "^18.0.2",
    "@delon/form": "^18.0.2",
    "@delon/mock": "^18.0.2",
    "@delon/theme": "^18.0.2",
    "@delon/util": "^18.0.2",
    "@ng-util/markdown": "^13.0.0",
    "@textbus/editor": "^2.1.6",
    "@types/mockjs": "^1.0.7",
    "@types/sm-crypto": "^0.3.4",
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "date-fns": "^2.28.0",
    "js-base64": "^3.7.2",
    "jsonc-parser": "3.0.0",
    "jszip": "3.10.1",
    "less": "4.1.3",
    "localforage": "^1.10.0",
    "mockjs": "^1.1.0",
    "ng-zorro-antd": "^18.0.1",
    "ngx-tinymce": "^14.1.1",
    "ngx-ueditor": "^14.0.0",
    "qrious": "^4.0.2",
    "rxjs": "~7.8.0",
    "screenfull": "^6.0.2",
    "simple-mind-map": "^0.6.16",
    "sm-crypto": "0.3.13",
    "ts-indexdb": "^0.0.6",
    "tslib": "^2.3.0",
    "uuid-int": "^3.1.0",
    "vditor": "^3.9.9",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.1",
    "@angular-eslint/builder": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^18.0.0",
    "@angular-eslint/eslint-plugin-template": "^18.0.0",
    "@angular-eslint/schematics": "^18.0.0",
    "@angular-eslint/template-parser": "^18.0.0",
    "@angular/cli": "^18.1.1",
    "@angular/compiler-cli": "^18.1.1",
    "@angular/language-service": "^18.0.0",
    "@delon/testing": "^18.0.2",
    "@ng-util/monaco-editor": "^18.0.0",
    "@types/jasmine": "^4.0.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "~2.25.3",
    "eslint-plugin-jsdoc": "~46.8.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-prettier": "~5.0.0",
    "husky": "^9.0.11",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "^29.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "lint-staged": "^12.1.4",
    "ng-alain": "^18.0.2",
    "ng-alain-plugin-theme": "^18.0.0",
    "ngx-tinymce": "^18.0.0",
    "node-fetch": "^2.6.1",
    "prettier": "^3.3.2",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^14.13.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.5.3"
  },
  "lint-staged": {
    "(src)/**/*.{html,ts}": [
      "eslint --fix"
    ],
    "(src)/**/*.less": [
      "pnpm run lint:style"
    ]
  },
  "packageManager": "pnpm@6.26.1"
}
